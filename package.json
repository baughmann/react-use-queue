    "queue",
    "job",
    "task",
    "background",
    "hook",
    "use",
    "usequeue"
  ],
  "author": "Nick Baughman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baughmann/react-use-queue/issues"
  },
  "homepage": "https://github.com/baughmann/react-use-queue#readme",
  "devDependencies": {
    "@types/react": "^16.8.0 || ^17 || ^18",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18"
  },
  "files": [
    "/lib"
  ]
